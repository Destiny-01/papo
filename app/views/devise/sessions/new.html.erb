<div class="row mt-5 float-center">
<div class="col"></div>
<div class="col">
  <div class="card "><h2 class="card-header text-center">Log In</h2>
    <div class="card-body mb-5">
      <%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
        <div class="form-inputs">
          <%= f.input :email,
                      required: false,
                      autofocus: true,
                      input_html: { autocomplete: "email" } %><br>
          <%= f.input :password,
                      required: false,
                      input_html: { autocomplete: "current-password" } %><br>
          <%= f.input :remember_me, as: :boolean if devise_mapping.rememberable? %>
        </div>

        <div class="form-actions">
          <%= f.button :submit, "Log in", class: "btn btn-primary" %>
        </div>
      <% end %>
    </div>
  <%# <br>
  <br>
  <br>
  <br> %>
  </div>
<br>

<%= render "devise/shared/links" %>

</div>
<div class="col"></div>
</div>